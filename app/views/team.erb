<div class="team">
<h2><%= @team.name %></h2>
<h4>Team Motto: <%=@team.motto%></h4>
</div>

<h2>Superheroes</h2>
<% @heroes.each do |hero|%>
    <div class="superhero">
    <h4>Hero Name: <%=hero.name%></h4>
    <p>Hero Power: <%=hero.power%></p>
    <p>Hero Biography: <%=hero.bio%></p>
    </div>
<%end%> 